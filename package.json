{
  "name": "homedo-cli",
  "description": "A command-line interface tool built for homedo.com",
  "version": "0.0.1",
  "author": "rmlzy <rmlzy@outlook.com> (https://poppython.com)",
  "main": "sao.js",
  "bin": "cli.js",
  "scripts": {
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "test": "ava",
    "test-coverage": "cross-env NODE_ENV=test nyc npm run test",
    "test:update-snap": "npm run test -- --update-snapshots"
  },
  "dependencies": {
    "cac": "^6.6.1",
    "chalk": "^4.1.1",
    "fixpack": "^3.0.6",
    "inquirer": "^8.0.0",
    "shelljs": "^0.8.4",
    "update-notifier": "^5.0.0"
  },
  "devDependencies": {
    "ava": "^3.13.0",
    "codecov": "^3.8.0",
    "cross-env": "^7.0.2",
    "nyc": "^15.1.0",
    "prettier": "^2.2.1",
    "xo": "^0.34.0"
  },
  "ava": {
    "failFast": true,
    "verbose": true,
    "files": [
      "test.js",
      "test-*.js",
      "test/**/*.js",
      "**/__tests__/**/*.js",
      "**/*.test.js",
      "template/test.js",
      "template/test-*.js",
      "template/test/**/*.js",
      "template/**/__tests__/**/*.js",
      "template/**/*.test.js",
      "!templates/test/helpers/**/*"
    ]
  },
  "engines": {
    "node": ">= 10"
  },
  "homepage": "https://github.com/rmlzy/homedo-cli",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/rmlzy/homedo-cli"
  },
  "xo": {
    "prettier": true,
    "space": true,
    "extends": [
      "xo-lass"
    ],
    "ignores": [
      "config.js"
    ]
  }
}
