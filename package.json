{
  "name": "homedo-cli",
  "description": "",
  "version": "0.0.1",
  "author": "rmlzy <rmlzy@outlook.com> (https://poppython.com)",
  "main": "sao.js",
  "bin": "cli.js",
  "scripts": {
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "lint": "npm run lint:js && npm run lint:md",
    "lint:js": "xo",
    "lint:md": "remark . -qfo",
    "pretest": "npm run lint",
    "test": "ava",
    "test-coverage": "cross-env NODE_ENV=test nyc npm run test",
    "test:update-snap": "npm run test -- --update-snapshots"
  },
  "dependencies": {
    "cac": "^6.6.1",
    "camelcase": "^6.1.0",
    "cross-spawn": "^7.0.3",
    "debug": "^4.2.0",
    "execa": "^4.0.3",
    "fixpack": "^3.0.6",
    "github-username": "^5.0.1",
    "github-username-regex": "^1.0.0",
    "global": "^4.4.0",
    "is-email": "^1.0.0",
    "is-url": "^1.2.4",
    "is-valid-npm-name": "^0.0.5",
    "npm-conf": "^1.1.3",
    "sao": "0.x",
    "semver": "^7.3.2",
    "shelljs": "^0.8.4",
    "spdx-license-list": "^6.3.0",
    "speakingurl": "^14.0.1",
    "superb": "^4.0.0",
    "update-notifier": "^5.0.0",
    "uppercamelcase": "^3.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "ava": "^3.13.0",
    "codecov": "^3.8.0",
    "cross-env": "^7.0.2",
    "eslint-config-xo-lass": "^1.0.4",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.2",
    "nyc": "^15.1.0",
    "prettier": "^2.2.1",
    "remark-cli": "^9.0.0",
    "remark-preset-github": "^3.0.3",
    "xo": "^0.34.0"
  },
  "ava": {
    "failFast": true,
    "verbose": true,
    "files": [
      "test.js",
      "test-*.js",
      "test/**/*.js",
      "**/__tests__/**/*.js",
      "**/*.test.js",
      "template/test.js",
      "template/test-*.js",
      "template/test/**/*.js",
      "template/**/__tests__/**/*.js",
      "template/**/*.test.js",
      "!templates/test/helpers/**/*"
    ]
  },
  "engines": {
    "node": ">= 10"
  },
  "homepage": "https://github.com/rmlzy/homedo-cli",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/rmlzy/homedo-cli"
  },
  "xo": {
    "prettier": true,
    "space": true,
    "extends": [
      "xo-lass"
    ],
    "ignores": [
      "config.js"
    ]
  }
}
